<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <property name="defaultPattern" value="%date %level %logger{36} - %msg%n" />

    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>${defaultPattern}</pattern>
        </encoder>
    </appender>

    <logger name="com.xunlei.youxi.proxy.dg" level="INFO">
    </logger>

    <root level="INFO">
        <appender-ref ref="console" />
    </root>
</configuration>
